{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.logodesignx.com"
        }
      ],
      "destination": "https://logodesignx.com/$1",
      "permanent": true
    }
  ],
  "routes": [
    {
      "src": "/category/uncategorized",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/sample-page/?",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/category/(.*)",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/wp-(.*)",
      "status": 301,
      "headers": { "Location": "/" }
    }
  ]
}
