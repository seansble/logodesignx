{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/category/uncategorized",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/sample-page/",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/sample-page",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/category/(.*)",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/wp-(.*)",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/author/(.*)",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/tag/(.*)",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    }
  ]
}
